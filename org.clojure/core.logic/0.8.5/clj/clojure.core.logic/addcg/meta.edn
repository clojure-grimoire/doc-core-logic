{:ns "clojure.core.logic", :name "addcg", :file "clojure/core/logic.clj", :type :fn, :src "(defn addcg [c]\n  (fn [a]\n    (let [a (reduce (fn [a x]\n                      (ext-no-check a x (subst-val ::unbound)))\n              a (unbound-rands a c))]\n      (assoc a :cs (addc (:cs a) a c)))))", :column 1, :line 1997, :arglists ([c])}