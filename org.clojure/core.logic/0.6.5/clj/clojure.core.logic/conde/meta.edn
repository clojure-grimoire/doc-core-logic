{:ns "clojure.core.logic", :name "conde", :file "clojure/core/logic.clj", :type :macro, :src "(defmacro conde [& clauses]\n  \"Logical disjunction of the clauses. The first goal in\n  a clause is considered the head of that clause. Interleaves the\n  execution of the clauses.\"\n  (let [a (gensym \"a\")]\n    `(fn [~a]\n       (-inc\n        (mplus* ~@(bind-conde-clauses a clauses))))))", :column 1, :line 915, :macro true, :arglists ([& clauses])}