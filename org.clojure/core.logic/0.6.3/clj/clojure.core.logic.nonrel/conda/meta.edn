{:ns "clojure.core.logic.nonrel", :name "conda", :file "clojure/core/logic/nonrel.clj", :type :macro, :src "(defmacro conda\n  [& clauses]\n  \"Soft cut. Once the head of a clause has succeeded\n  all other clauses will be ignored. Non-relational.\"\n  (let [a (gensym \"a\")]\n    `(fn [~a]\n       (ifa* ~@(map (cond-clauses a) clauses)))))", :column 1, :line 112, :macro true, :arglists ([& clauses])}